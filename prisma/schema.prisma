generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Categories {
  id Int @id @default(autoincrement())
}

model Description {
  id          Int      @id @default(autoincrement())
  userId      String?
  title       String?
  userName    String?
  mainNumber  String?
  phoneNumber String?
  uploadDate  String?
  country     String?
  createdAt   DateTime @default(now()) @db.DateTime(0)
  imageURL    String?
  fish        String?
  tag         String?
  desc        String?  @db.LongText
}

model User {
  id      Int    @id @default(autoincrement())
  desc    String
  country String
  fishs   String
}
